log = std:log:sublog c++

# Alias `std:fs:track` to change `force-check` in tests.
track = fn :file -> std:fs:track (std:Type:any :force-check = std:dynamic:get test) $file

## Commands
##
## * `test` - run the test suite
command = std:match:value [
    fn test -> {
        (!id ~set=std:Bool:false std:dynamic:eval) { test = () } <| std:test:run {
            module = std:load-lazy src/module.test
        }
    }
]
